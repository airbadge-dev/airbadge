{
  "name": "@airbadge/sveltekit",
  "version": "0.0.6",
  "keywords": [
    "svelte",
    "auth.js",
    "stripe",
    "saas"
  ],
  "description": "A SaaS module for Auth.js. It adds Stripe Subscriptions and Checkout.",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "dotenv -e .env.test -- playwright test",
    "test:unit": "dotenv -e .env.test -- vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./client.js": {
      "types": "./dist/client.d.ts",
      "svelte": "./dist/client.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@auth/prisma-adapter": "^2.1.0",
    "@faker-js/faker": "^8.4.0",
    "@playwright/test": "^1.28.1",
    "@prisma/client": "^5.9.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prisma": "^5.9.0",
    "publint": "^0.1.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@auth/sveltekit": "^1.1.0",
    "stripe": "^14.14.0",
    "@sveltejs/kit": "^2.0.6"
  },
  "prisma": {
    "seed": "node ./prisma/seed.js"
  }
}
